    "MarkdownPaste.path": "${fileWorkspaceFolder}/images/${fileBasenameNoExtension}/",
    
    "MarkdownPaste.namePrefix":"",//文件名的前缀
    "MarkdownPaste.nameBase":"${datetime}",//文件名
    "MarkdownPaste.nameSuffix":"",//文件名附加字符串

    "MarkdownPaste.silence":false, //粘贴时的确认框
    
}