{
    "pasteImage.path":"${currentFileDir}/images/HTML5重学笔记/",//剪切板图片保存路径 A
    "pasteImage.defaultName":"Y-MM-DD-HH-mm-ss",//图片名称

    "pasteImage.basePath":"${currentFileDir}/",//上面的路径A减去这里设置的路径，就是md文件![](这里的路径)
