{
  "author": "Yigui-Ding",
  "license": "MIT",
  "keywords": [
    "personal-blog"
  ],
  "type": "module",
  "scripts": {
    "submodule_install": "git submodule update --init --recursive",
    "vp-update": "npx vp-update",
    "ncu": "npx npm-check-updates -u",
    "dev": "vuepress dev blog",
    "dev:clean-dev": "vuepress dev blog --clean-cache",
    "build": "vuepress build blog",
    "lint": "prettier --check --write .",
    "lint:check": "prettier --check .",
    "lint-md": "markdownlint **/*.md --ignore node_modules --fix",
    "commitizen_install": "npm install commitizen -D",
    "commitizen_init": "commitizen init cz-conventional-changelog --save --save-exact",
    "commit": "git cz",
    "publish:save": "git add . && git commit -m 'update'",
    "publish": "git add . && git commit -m 'update' && git push --force origin master"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "^2.0.0-rc.9",
    "@vuepress/plugin-feed": "^2.0.0-rc.26",
    "@vuepress/plugin-pwa": "^2.0.0-rc.26",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "execa": "^8.0.1",
    "less": "^4.2.0",
    "markdownlint-cli": "^0.39.0",
    "mermaid": "^10.9.0",
    "npm-check-updates": "^16.14.20",
    "postcss-preset-env": "^9.5.9",
    "prettier": "^3.2.5",
    "vuepress": "2.0.0-rc.9",
    "vuepress-plugin-search-pro": "^2.0.0-rc.37",
    "vuepress-theme-hope": "2.0.0-rc.37"
  },
  "dependencies": {
    "@vue/repl": "^4.1.2",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "element-plus": "^2.7.2",
    "katex": "^0.16.10",
    "nodemon": "^3.1.0",
    "spacecraft.io": "^0.0.4",
    "vp-update": "^2.0.0-rc.15",
    "vue": "^3.4.25",
    "vue3-live2d": "^0.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
